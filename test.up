SayHello {
  name : String("");

  (abc:String) {
    this.name = abc;
  }

  Say() {
    ("Ciao " + this.name + "!").Print();
  }

  Assign(src: SayHello) {
    this.name = src.name;
  }

}

c : Number(0);
