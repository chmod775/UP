// Using basic types
m : Int;
r : Float = 3.1415;

m = 775;

print(m);
print(r);


// Using global types classes (String and Number)
a : String = "Hello";
b : String = "World!";
pi : Number : 3.1415;

print(pi);
print(a + " " + b);
print("Value of $1 is: $2", a, pi);
print('Value of {a} is: {pi}');

c : String;
c = '{a} {b} {pi}';
print(c);